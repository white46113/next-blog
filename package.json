{
  "name": "turborepo-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:anime-blog": "cd apps/anime-blog && npm run dev",
    "dev:tech-blog": "cd apps/tech-blog && npm run dev",
    "dev:tool": "cd apps/tool && npm run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "test": "turbo run test"
  },
  "devDependencies": {
    "turbo": "^1.13.3",
    "@tailwindcss/typography": "^0.5.10"
  },
  "packageManager": "npm@10.3.0",
  "engines": {
    "node": "20.11.1",
    "npm": "10.3.0"
  }
}